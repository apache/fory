package auto_id;

message Envelope {
    string id = 1;

    message Payload {
        int32 value = 1;
    }

    union Detail {
        Payload payload = 1;
        string note = 2;
    }

    Payload payload = 2;
    Detail detail = 3;
}

union Wrapper {
    Envelope envelope = 1;
    string raw = 2;
}
