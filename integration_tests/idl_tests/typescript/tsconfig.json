{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["test/**/*", "generated/**/*", "roundtrip.ts"]
}
